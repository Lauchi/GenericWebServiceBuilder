DomainClass User {
	Id: Guid;
	Name: String;
	Age: Int32;

	Create ( Name: String) : UserCreateEvent;
	UpdateAge ( Age: Int32) : UpdateAgeEvent;
	UpdateName ( Name: String) : UpdateNameEvent;

	Posts: [Post];
}

DomainClass Post {
	Id: Guid;
	Title: String;
	Body: String;

	Create ( Name: String): PostCreateEvent;
}

DomainEvent UserCreateEvent {
	UserId: Guid;
}

DomainEvent UpdateAgeEvent {
	UserId: Guid;
	Age: Int32;
}

DomainEvent UpdateNameEvent {
	UserId: Guid;
	Name: String;
}