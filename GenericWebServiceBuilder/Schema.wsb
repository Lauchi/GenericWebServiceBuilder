DomainClass User {
	Id: Guid
	Name: String
	Age: Int32

	Create(Name: String): {
		UserId: Guid
	}

	UpdateAge(Age: Int32): {
		UserId: Guid
		Age: Int32
	}

	UpdateName(Name: String): {
		UserId: Guid
		Name: String
	}

	Posts: [Post];
}

DomainClass Post {
	Id: Guid;
	Title: String;
	Body: String;

	DomainMethod Create(Title: String): {
		UserId: Guid
		Title: String
	}
}